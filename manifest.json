{
  "manifest_version": 2,
  "name": "Harbor Master",
  "description": "Check out who has the pull requests, and who should have pull requests.",
  "version": "0.0.2",
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}